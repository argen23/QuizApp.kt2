<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quizappkt2">

    <application
        android:allowBackup="true"
        android:icon="@drawable/artut"
        android:label="@string/app_name"
        android:roundIcon="@drawable/artut"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuizAppkt2">

        <activity android:name=".kolg.ui.SplashScreen"
            android:theme = "@style/Theme.SplashScreenTheme"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".kolg.ui.MainActivity"
            android:exported="true"
            android:label="@string/app_name">
        </activity>
    </application>

</manifest>